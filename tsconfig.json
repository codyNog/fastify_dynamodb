{
  "compilerOptions": {
    "lib": [
      "dom",
      "es6",
      "es2017",
      "esnext.asynciterable"
    ],
    "sourceMap": true,
    "outDir": "./dist",
    "target": "es5", // TSはECMAScript 5に変換
    "module": "commonjs", // TSのモジュールはES Modulesとして出力,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./src/*"]
    },
    "moduleResolution": "node",
  }
}